<div id="<%= dom_id product %>">
  <div>
    <strong>Name:</strong>
    <%= product.name %>
  </div>

  <div>
    <strong>Steam app:</strong>
    <%= product.steam_app_id %>
  </div>

  <div>
    <strong>Price currency:</strong>
    <%= product.price_currency %>
  </div>

  <div>
    <strong>Steam price cents:</strong>
    <%= product.steam_price_cents %>
  </div>

</div>
